    "def compute(expression):\n",
    "    values = expression.split(' ')\n",
    "    num0 = int(values[0])\n",
    "    operator = values[1]\n",
    "    num1 = int(values[2])\n",
    "    if operator == '+':\n",
    "        return num0 + num1\n",
    "    else:\n",
    "        print('unknown operator!')\n",
    "        return None"
